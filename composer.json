{
    "name": "wittylizard",
    "license": "proprietary",
    "type": "project",
    "description": "Web server for wittylizard.com",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "sensio/framework-extra-bundle": "^5.2@dev",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/monolog-bundle": "^3.0@dev",
        "symfony/orm-pack": "dev-master",
        "symfony/security": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/swiftmailer-bundle": "^3.2@dev",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/webpack-encore-pack": "dev-master",
        "symfony/yaml": "^4.1"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0@dev",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.0@dev",
        "symfony/profiler-pack": "dev-master",
        "symfony/web-server-bundle": "^4.1"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "post-deploy-cmd": [
            "chmod -R ug+w $APP_DIR/var && ln -s $APP_DIR/src/Resources/css $APP_DIR/public/css && ln -s $APP_DIR/src/Resources/js $APP_DIR/public/js"
        ],
        "auto-scripts": {

        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "id": "01C7NFJ7STVJB7PZNNMTZHYT5Q",
            "allow-contrib": false
        }
    }
}
